
public class Test1 {

	public static void main(String[] args) {
		
		Thread t  = new Mythread();
		
		System.out.println(Thread.currentThread().getName());
		
	}
}


class Mythread extends Thread
{
	public void run()
	{
		System.out.println(Thread.currentThread().getName());
		
		System.out.println("This is task");
	}
}