public class Test4 {

	public static void main(String[] args) {

		Result result = new Result();
		
		
		Thread t1 = new MyThread(result,10);
		Thread t2 = new MyThread(result,20);
		Thread t3 = new MyThread(result,30);

		t1.start();
		t2.start();
		t3.start();
	}
}

class MyThread extends Thread {
	
	private Result result;
	private String number;

	public MyThread(Result result , String number) {

		this.result = result;
		this.number = number;
	}


	@Override
	public void run() {
		
		result.printResult(Thread.currentThread().getName() + "result" + result.printResult( ));
	
	}
}

class Result {
	public String printResult(String n) {
		System.out.println("Marks is " + n);
	}

}