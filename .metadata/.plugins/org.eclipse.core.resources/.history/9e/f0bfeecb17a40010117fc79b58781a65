public class Test {

	public static void main(String[] args) {
		
		Thread m =  Thread.currentThread();
		System.out.println(m.getName());
		
		
		Thread t1 = new MyThread();
		t1.start();
	}
	
}

class MyThread extends Thread
{
	public void run()
	{
		
		Thread.currentThread();
		
		System.out.println("this is task");
	}
	
}
