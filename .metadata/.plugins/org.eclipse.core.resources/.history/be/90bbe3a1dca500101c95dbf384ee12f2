public class Test4 {

	public static void main(String[] args) {

		Result result = new Result();
		
		
		Thread t1 = new MyThread(result,"as");
		Thread t2 = new MyThread(result,"as");
		Thread t3 = new MyThread(result,"aas");

		t1.start();
		t2.start();
		t3.start();
	}
}

class MyThread extends Thread {
	
	private Result result;
	private String number;

	public MyThread(Result result , String number) {

		this.result = result;
		this.number = number;
	}


	@Override
	public void run() {
		
	System.out.println(Thread.currentThread().getName());
	result.printResult(number);
	}
}

class Result {
	public void printResult(String n) {
		System.out.println(Thread.currentThread().getName()"Marks is " + n);
	}

}