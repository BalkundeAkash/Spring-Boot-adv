public class Test4 {

	public static void main(String[] args) {

		Result result = new Result();
		
		
		Thread t1 = new MyThread(result);
		Thread t2 = new MyThread(result);
		Thread t3 = new MyThread(result);

	}
}

class MyThread extends Thread {
	
	private Result result;

	public MyThread(Result result) {

		this.result = result;
	}


	@Override
	public void run() {
		
		result.printResult(10);
	
	}
}

class Result {
	public void printResult(int n) {
		System.out.println("Marks is " + n);
	}

}